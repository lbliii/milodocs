{
  "build": {
    "env": {
      "HUGO_VERSION": "0.146.0"
    }
  },
  "installCommand": "curl -L https://github.com/gohugoio/hugo/releases/download/v0.146.0/hugo_extended_0.146.0_linux-amd64.tar.gz | tar -xz && mv hugo /usr/local/bin/",
  "buildCommand": "ls -la && cd exampleSite && hugo --environment production --minify --theme ../.. --logLevel debug",
  "outputDirectory": "exampleSite/public",
  "framework": null
}